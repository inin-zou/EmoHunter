# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Optional: Custom owner addresses (if not provided, uses defaults)
OWNER_1=0xYourOwner1Address
OWNER_2=0xYourOwner2Address
OWNER_3=0xYourOwner3Address
OWNER_4=0xYourOwner4Address

# Optional: Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key

# Optional: RPC URLs (if not using defaults)
SEPOLIA_RPC_URL=https://rpc.sepolia.org
MAINNET_RPC_URL=https://eth.llamarpc.com